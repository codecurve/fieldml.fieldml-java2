    </operations>
  </ContinuousCompositeEvaluator>
  

  <PiecewiseField meshDomain="test_mesh.domain" name="test_mesh.coordinates.x" valueDomain="library.co-ordinates.rc.1d">
    <evaluatorList>
      <BilinearQuad dofs="test_mesh.point.x" dofIndexes="test_mesh.quad_nodes" name="bilinear_quad" />
    </evaluatorList>
    <elementEvaluators>
      <SimpleMapEntry key="1" value="bilinear_quad" />
