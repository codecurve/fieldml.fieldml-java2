      <MapEntry>
        <keys>7  </keys>
        <ContinuousDomainValue>
          <values>30.0  </values>
        </ContinuousDomainValue>
      </MapEntry>
    </entries>
  </ContinuousParameters>
  

  <ContinuousParameters name="test_mesh.point.y" valueDomain="library.co-ordinates.rc.1d">
    <parameterDomains>"test_mesh.global_dofs"  </parameterDomains>
    <entries>
      <MapEntry>
        <keys>1  </keys>
        <ContinuousDomainValue>
          <values>20.0  </values>
