  <ContinuousCompositeEvaluator name="test_mesh.point.x" valueDomain="library.co-ordinates.rc.1d">
    <parameterDomains>"test_mesh.nodes"  </parameterDomains>
    <operations>
      <MappedImportOperation sourceField="test_mesh.point.x" weightings="test_mesh.p2nAMap" iteratedDomain="test_mesh.points" />
    </operations>
  </ContinuousCompositeEvaluator>
  

  <ContinuousCompositeEvaluator name="test_mesh.point.y" valueDomain="library.co-ordinates.rc.1d">
