  <MapEvaluator valueIndexes="test_mesh.quad_nodes" valueWeights="test_mesh.mesh.bilinear_lagrange" valueSource="test_mesh.local_dofs" name="test_mesh.element.bilinear_lagrange" valueDomain="library.co-ordinates.rc.1d" />
  

  <ContinuousAggregateEvaluator name="test_mesh.coordinates.xy" valueDomain="library.co-ordinates.rc.2d">
    <sourceFields>"test_mesh.coordinates.x"  "test_mesh.coordinates.y"  </sourceFields>
  </ContinuousAggregateEvaluator>
  

  <PiecewiseTemplate name="test_mesh.coordinates.template" meshDomain="test_mesh.domain">
    <elementMaps>
      <TemplateMap element="1" evaluator="test_mesh.element.bilinear_lagrange" />
      <TemplateMap element="2" evaluator="test_mesh.element.bilinear_lagrange" />
      <TemplateMap element="3" evaluator="test_mesh.element.bilinear_lagrange" />
    </elementMaps>
  </PiecewiseTemplate>
  

  <PiecewiseField template="test_mesh.coordinates.template" name="test_mesh.coordinates.x" valueDomain="library.co-ordinates.rc.1d">
    <variables>
      <SimpleMapEntry key="test_mesh.dofs" value="test_mesh.point.x" />
    </variables>
  </PiecewiseField>
  

  <PiecewiseField template="test_mesh.coordinates.template" name="test_mesh.coordinates.y" valueDomain="library.co-ordinates.rc.1d">
    <variables>
      <SimpleMapEntry key="test_mesh.dofs" value="test_mesh.point.y" />
    </variables>
  </PiecewiseField>
  

