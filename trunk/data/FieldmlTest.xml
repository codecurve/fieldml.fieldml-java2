<?xml version="1.0" encoding="UTF-8"?>
<fieldml>
  <!--
1___2___3
|   |2 /|
| 1 | / |
|   |/ 3|
4___5___6
-->
  <ContinuousDomain dimensions="2" name="global.plane" />
  

  <EnsembleDomain name="test_mesh.nodes">
    <values>1  2  3  4  5  6  </values>
  </EnsembleDomain>
  

  <ContinuousDomain dimensions="1" name="global.line" />
  

  <EnsembleDomain name="global.quad.local_node">
    <values>1  2  3  4  </values>
  </EnsembleDomain>
  

  <ContinuousDomain dimensions="1" name="global.temperature" />
  

  <EnsembleDomain name="global.triangle.local_node">
    <values>1  2  3  </values>
  </EnsembleDomain>
  

  <EnsembleDomain name="test_mesh.elements">
    <values>1  2  3  </values>
  </EnsembleDomain>
  

  <MeshDomain discretization="test_mesh.elements" dimensions="2" name="test_mesh.domain" />
  

  <MappingField name="test_mesh.node.x" valueDomain="global.line">
    <parameterDomains>"test_mesh.nodes"  </parameterDomains>
    <entries>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>0.0  </chartValues>
        </ContinuousDomainValue>
        <keys>1  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>10.0  </chartValues>
        </ContinuousDomainValue>
        <keys>2  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>20.0  </chartValues>
        </ContinuousDomainValue>
        <keys>3  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>0.0  </chartValues>
        </ContinuousDomainValue>
        <keys>4  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>10.0  </chartValues>
        </ContinuousDomainValue>
        <keys>5  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>20.0  </chartValues>
        </ContinuousDomainValue>
        <keys>6  </keys>
      </MapEntry>
    </entries>
  </MappingField>
  

  <MappingField name="test_mesh.node.y" valueDomain="global.line">
    <parameterDomains>"test_mesh.nodes"  </parameterDomains>
    <entries>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>10.0  </chartValues>
        </ContinuousDomainValue>
        <keys>1  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>10.0  </chartValues>
        </ContinuousDomainValue>
        <keys>2  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>10.0  </chartValues>
        </ContinuousDomainValue>
        <keys>3  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>0.0  </chartValues>
        </ContinuousDomainValue>
        <keys>4  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>0.0  </chartValues>
        </ContinuousDomainValue>
        <keys>5  </keys>
      </MapEntry>
      <MapEntry>
        <ContinuousDomainValue>
          <chartValues>0.0  </chartValues>
        </ContinuousDomainValue>
        <keys>6  </keys>
      </MapEntry>
    </entries>
  </MappingField>
  

  <MappingField name="test_mesh.quad_nodes" valueDomain="test_mesh.nodes">
    <parameterDomains>"test_mesh.elements"  "global.quad.local_node"  </parameterDomains>
    <entries>
      <MapEntry>
        <EnsembleDomainValue indexValue="4" />
        <keys>1  1  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="5" />
        <keys>1  2  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="1" />
        <keys>1  3  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="2" />
        <keys>1  4  </keys>
      </MapEntry>
    </entries>
  </MappingField>
  

  <MappingField name="test_mesh.triangle_nodes" valueDomain="test_mesh.nodes">
    <parameterDomains>"test_mesh.elements"  "global.triangle.local_node"  </parameterDomains>
    <entries>
      <MapEntry>
        <EnsembleDomainValue indexValue="2" />
        <keys>2  1  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="5" />
        <keys>2  2  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="3" />
        <keys>2  3  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="6" />
        <keys>3  1  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="3" />
        <keys>3  2  </keys>
      </MapEntry>
      <MapEntry>
        <EnsembleDomainValue indexValue="5" />
        <keys>3  3  </keys>
      </MapEntry>
    </entries>
  </MappingField>
  

  <NodeDofEvaluator nodeDofs="test_mesh.node.y" elementNodes="test_mesh.quad_nodes" interpolation="library::quad_bilinear" name="test_mesh.evaluator.y.quad_bilinear" />
  

  <NodeDofEvaluator nodeDofs="test_mesh.node.x" elementNodes="test_mesh.quad_nodes" interpolation="library::quad_bilinear" name="test_mesh.evaluator.x.quad_bilinear" />
  

  <NodeDofEvaluator nodeDofs="test_mesh.node.x" elementNodes="test_mesh.triangle_nodes" interpolation="library::triangle_bilinear" name="test_mesh.evaluator.x.simplex_bilinear" />
  

  <NodeDofEvaluator nodeDofs="test_mesh.node.y" elementNodes="test_mesh.triangle_nodes" interpolation="library::triangle_bilinear" name="test_mesh.evaluator.y.simplex_bilinear" />
  

  <FEMField meshDomain="test_mesh.domain" name="test_mesh.coordinates.x" valueDomain="global.line">
    <evaluators>
      <MapEntry key="1" evaluator="test_mesh.evaluator.x.quad_bilinear" />
      <MapEntry key="2" evaluator="test_mesh.evaluator.x.simplex_bilinear" />
      <MapEntry key="3" evaluator="test_mesh.evaluator.x.simplex_bilinear" />
    </evaluators>
  </FEMField>
  

  <FEMField meshDomain="test_mesh.domain" name="test_mesh.coordinates.y" valueDomain="global.line">
    <evaluators>
      <MapEntry key="1" evaluator="test_mesh.evaluator.y.quad_bilinear" />
      <MapEntry key="2" evaluator="test_mesh.evaluator.y.simplex_bilinear" />
      <MapEntry key="3" evaluator="test_mesh.evaluator.y.simplex_bilinear" />
    </evaluators>
  </FEMField>


</fieldml>

