  <EnsembleParameters name="test_mesh.element_dof_indexes" valueDomain="test_mesh.dof_indexes">
    <ParameterTable />
  </EnsembleParameters>
  

  <PiecewiseField template="test_mesh.coordinates" name="test_mesh.coordinates.z" valueDomain="library.co-ordinates.rc.1d">
